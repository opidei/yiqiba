.UCenter-bg {
  background-color: white;
  background-size: cover;
  height: 650rpx;
  display: flex;
  justify-content: center;
  padding-top: 60rpx;
  overflow: hidden;
  position: relative;
  flex-direction: column;
  align-items: center;
  color: #333;
  font-weight: 300;
  text-shadow: 0 0 1px rgba(0, 0, 0, 0.1);
}

.UCenter-bg text {
  opacity: 0.8;
}

.UCenter-bg image {
  width: 200rpx;
  height: 200rpx;
}

.UCenter-bg .gif-wave{
  position: absolute;
  width: 100%;
  bottom: 0;
  left: 0;
  z-index: 99;
  mix-blend-mode: screen;  
  height: 100rpx;   
}

map,.mapBox{
  left: 0;
  z-index: 99;
  mix-blend-mode: screen;  
  height: 100rpx;   
}

map,.mapBox{
  width: 750rpx;
  height: 300rpx;
}
.cu-form-group .title {
  min-width: calc(4em + 30rpx);
}